@{
    PaginationModel pagination = new PaginationModel();

    int pageNo = 0;

    if (ViewBag.Pagination != null)
    {
        pagination = ViewBag.Pagination;
        pageNo = pagination.CurrentPage;
    }
}
@{
    Layout = "_navbar2";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <link rel="stylesheet" href="~/css/Volunteering.css" asp-append-version="true" />

</head>
<body>
    <div class="container  missioncontainer">
        <div class="row">
            <div class="leftdiv  col-12 col-lg-6 col-md-12 col-sm-12  ">

                <div class="container  shadow-lg p-0">

                    <!-- Full-width images with number text -->
                    <div class="mySlides">
                        @*<div class="numbertext">1 / 6</div>*@
                        <img src="~/images/CSR-initiative-stands-for-Coffee--and-Farmer-Equity-3.png" style="width:100%">
                    </div>

                    <div class="mySlides">
                        @*<div class="numbertext">2 / 6</div>*@
                        <img src="~/images/img1.png" style="width:100%">
                    </div>

                    <div class="mySlides">
                        @*<div class="numbertext">3 / 6</div>*@
                        <img src="~/images/Grow-Trees-On-the-path-to-environment-sustainability-2.png" style="width:100%">
                    </div>

                    <div class="mySlides">
                        @*<div class="numbertext">4 / 6</div>*@
                        <img src="~/images/img11.png" style="width:100%">
                    </div>

                    @*                    <div class="mySlides">
                    @*<div class="numbertext">5 / 6</div>
                    <img src ="~/images/img22.png"  style="width:100%">
                    </div>

                    <div class="mySlides">
                    @*<div class="numbertext">6 / 6</div>*@
                    @*   <img src="~/images/img.png" style="width:100%">
                    </div>*@

                    <!-- Next and previous buttons -->
                    <!-- Thumbnail images -->
                    <div class="row mt-2  p-0">

                        <a class="prev " onclick="plusSlides(-1)">&#10094;</a>
                        <a class="next" onclick="plusSlides(1)">&#10095;</a>
                        <div class="column pe-0  " style="width:25%">
                            <img class="demo cursor " src="~/images/CSR-initiative-stands-for-Coffee--and-Farmer-Equity-1.png" style="width:100%" onclick="currentSlide(1)" alt="The Woods">
                        </div>
                        <div class="column  px-0 " style="width:25%">
                            <img class="demo cursor " src="~/images/img1.png" style="width:100%" onclick="currentSlide(2)" alt="Cinque Terre">
                        </div>
                        <div class="column px-0  " style="width:25%">
                            <img class="demo cursor  " src="~/images/Grow-Trees-On-the-path-to-environment-sustainability-1.png" style="width:100%" onclick="currentSlide(3)" alt="Mountains and fjords">
                        </div>
                        <div class="column ps-0 " style="width:25%">
                            <img class="demo cursor " src="~/images/img11.png" style="width:100%" onclick="currentSlide(4)" alt="Northern Lights">
                        </div>

                    </div>
                </div>


            </div>
            <div class="rightdiv  col-12 col-lg-6 col-md-12 col-sm-12">
                <div class="card border-0">
                    <div class="card-body">
                        <h5 class="card-title"> @ViewBag.Missiondetail.Title</h5>
                        <h6 class="card-subtitle mb-2 text-muted">@ViewBag.Missiondetail.ShortDescription</h6>


                        @if (@ViewBag.Missiondetail.MissionType == "Time")
                        {
                            <div class=" col-12 rule d-flex mt-1">
                                <hr class="trees">
                                <div class="goal col-8 col-sm-4 rounded-5  text-center px-2  " style="width:max-content;">
                                    From @ViewBag.Missiondetail.StartDate Until @ViewBag.Missiondetail.EndDate
                                </div>
                                <hr class="trees">
                            </div>

                        }
                        else
                        {
                            <div class=" col-12 rule d-flex mt-1">
                                <hr class="trees">
                                <div class="goal col-8 col-sm-4 rounded-5  text-center ">
                                    @ViewBag.Missiondetail.GoalObjectiveText
                                </div>
                                <hr class="trees">
                            </div>
                        }


                        <div class="d-flex  mt-1" id="seatleft">
                            <div class="d-flex me-auto  col-6">
                                <img src="~/images/Seats-left.png" alt="" class="mt-1 ms-1">
                                <div class="ms-4 ">
                                    <fs-2>@ViewBag.Missiondetail.Availability</fs-2><br>
                                    @if (@ViewBag.Missiondetail.isApplied == true && @ViewBag.Missiondetail.UserId != null)
                                    {
                                        <fs-5> Alredy Applied</fs-5>
                                    }
                                    else
                                    {

                                        <fs-5> seats left</fs-5>
                                    }
                                </div>
                            </div>
                            @if (@ViewBag.Missiondetail.MissionType == "Time")
                            {
                                <div class="d-flex col-6">
                                    <img src="~/images/deadline.png" alt="" class="mt-1 ms-1">
                                    <div class="ms-3 ">
                                        <fs-2>@ViewBag.Missiondetail.EndDate</fs-2><br>
                                        <fs-5>Deadline</fs-5>
                                    </div>
                                </div>
                            }
                            else
                            {
                                <div class="d-flex me-3 col-6">
                                    <img src="~/images/achieved.png" alt="" class="mt-2">
                                    <div class="ms-2 ">
                                        <div class="progress mt-2" role="progressbar" aria-label="Example with label" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
                                            <div class="progress-bar bg-warning" style="width: 25%">25%</div>
                                        </div>
                                        <fs-5>Deadline</fs-5>
                                    </div>
                                </div>
                            }
                        </div>

                        <hr class="" />



                        <div class="row p-1 mx-auto">
                            <div class="col  col-11  col-xl-5 col-lg-11 col-md-5 col   me-xl-4  me-md-4 rounded-5 d-flex   Addfavourite   justify-content-center" onclick="addtofav1(@ViewBag.Missiondetail.MissionId,@ViewBag.UserId)">
                                <i class="bi bi-heart-fill" id="heart" style="@(@ViewBag.Missiondetail.isFavriout ? "color:#F88634;" : "color:black;")"></i>

                                @if (@ViewBag.Missiondetail.isFavriout == true)
                                {
                                    <label class="ms-2" id="favmissiondiv" style="@(@ViewBag.Missiondetail.isFavriout ? "color:#F88634;" : "color:black;")">
                                        Remove from Favourite
                                    </label>
                                }
                                else
                                {
                                    <label class="ms-2" id="favmissiondiv" style="@(@ViewBag.Missiondetail.isFavriout ? "color:#F88634;" : "color:black;")">
                                        ADD to Favourite
                                    </label>
                                }


                            </div>
                            <button class="col col-11 col-xl-6 col-lg-11  col-md-6 col-sm-11 rounded-5 d-flex bg-white  mt-2 mt-xl-0 mt-lg-2 mt-md-0  Addfavourite   justify-content-center align-items-center" type="button" data-bs-toggle="modal" data-bs-target="#exampleModal">

                                <img src="~/images/add1.png" class="" />


                                <label class="ms-2"> Recommend to a Co-Worker</label>

                            </button>

                            <div class="modal fade  col-6" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                <div class="modal-dialog modal-dialog-centered">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h1 class="modal-title fs-5" id="exampleModalLabel">Send Recomandation to Friend</h1>
                                            <button type="button" class=" btn border-0 bg-white fw-bold  " data-bs-dismiss="modal" aria-label="Close">
                                                <i class="bi bi-x-lg "></i>
                                            </button>
                                        </div>
                                        <div class="modal-body overflow-y-auto " style="height:200px;">
                                            @foreach (var user in ViewBag.allavailuser)
                                            {
                                                <div class=" userlistdiv align-items-center d-flex my-3">
                                                    @if (@user.Useravtar != "")
                                                    {
                                                        <img src="@user.Useravtar" class="aviluser ms-2 my-1">
                                                    }
                                                    else
                                                    {
                                                        <img src="~/images/user1.png" class="aviluser ms-2 my-1">
                                                    }
                                                    <label class=" ms-4  me-auto">   @user.username  @user.lastname </label>
                                                    <input class="form-check-input me-2 " type="checkbox" value="" id="@user.userEmail" name="email">
                                                </div>
                                            }
                                        </div>
                                        <div class="modal-footer">

                                            <button type="button" class="btn"  style="background-color:#F88634;border-color:#F88634;" id="sendbutton" onclick="sendRecom(@ViewBag.Missiondetail.MissionId)">Send Recomandation</button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                       
                        <div class=" row mt-1">
                            <div class="col-12 Ratting d-flex" id="Ratting">
                                <hr class="trees" />
                                @if (@ViewBag.Missiondetail.Givenrating != null)

                                {
                                    for (long i = 0; i < @ViewBag.Missiondetail.Givenrating; i++)
                                    {
                                        var k = i + 1;
                                        <i class="bi bi-star-fill" id="@k" style="color:#F88634; " onclick="addRating(this.id,@ViewBag.Missiondetail.MissionId,@ViewBag.UserId)"></i>
                                    }
                                    for (long i = @ViewBag.Missiondetail.Givenrating; i < 5; i++)
                                    {
                                        var k = i + 1;
                                        <i class="bi bi-star-fill" id="@k" style=" " ; border-color:black" onclick="addRating(this.id,@ViewBag.Missiondetail.MissionId,@ViewBag.UserId)"></i>
                                    }


                                }


                                <hr class="trees" />
                            </div>

                        </div>
                        <div class="row justify-content-center  p-2 mt-2">
                            <div class="col col-5 col-xl-3 d-xl-block d-lg-none  col-md-3 col-sm-5     rounded-1 foursquer ">
                                <img src="~/images/pin1.png" alt="" class="" />
                                <br />
                                <label class="text-muted mt-4">city</label><br />
                                <label class=""> @ViewBag.Missiondetail.Cityname</label>

                            </div>

                            <div class="col col-5  col-xl-3  d-xl-block d-lg-none col-md-3 col-sm-5  rounded-1 foursquer">
                                <img src="~/images/web.png" alt=" " class="" />
                                <br />
                                <label class="text-muted mt-4">Theme</label><br />
                                <label class="">  @ViewBag.Missiondetail.Themename</label>
                            </div>
                            <div class="col col-5 col-xl-3 d-xl-block d-lg-none col-md-3  col-sm-5  mt-3  mt-xl-0 mt-lg-3 mt-md-0  rounded-1 foursquer">
                                <img src="~/images/calender.png" alt=" " class="" />
                                <br />
                                <label class="text-muted mt-4">Date  </label><br />
                                <label class="">  Ongoing Opportunity</label>
                            </div>
                            <div class="col col-5 col-xl-3 d-xl-block d-lg-none col-md-3  col-sm-5      mt-3  mt-xl-0 mt-lg-3 mt-md-0 rounded-1 foursquer">
                                <img src="~/images/organization.png" alt=" " class="" />
                                <br />
                                <label class="text-muted mt-4">Organization  </label><br />
                                <label class="">@ViewBag.Missiondetail.OrganizationName</label>
                            </div>


                        </div>
                            <div class="row justify-content-center Appleddiv ">
                        @if (ViewBag.Missiondetail.isApplied == true)
                            {
                                   <button class="aply  col-8 col-xl-4 d-xl-block d-lg-none  col-md-4  mt-3   d-flex rounded-5 bg-white px-4 py-2 justify-content-center ">
                                    <a class="aplys text-decoration-none">
                                        Alredy Applied

                                    </a>
                                </button>


                            }
                            else if (ViewBag.Missiondetail.isPendding == true)
                            {
                                <button class="aply  col-8 col-xl-4 d-xl-block d-lg-none  col-md-4  mt-3   d-flex rounded-5 bg-white px-4 py-2 justify-content-center ">
                                    <a class="aplys text-decoration-none">
                                      Applied Pendding

                                    </a>
                                </button>
                            }else
                            {



                                <button class="aply  col-8 col-xl-4 d-xl-block d-lg-none  col-md-4  mt-3   d-flex rounded-5 bg-white px-4 py-2 justify-content-center " onclick="missionapplied(@ViewBag.Missiondetail.MissionId,@ViewBag.UserId)">
                                    <a class="aplys text-decoration-none">
                                        Apply Now
                                        <img src="~/images/right-arrow.png" class="mt-0 ms-2" />
                                    </a>
                                </button>

                             
                           

                        }
                            </div>

                    </div>
                </div>
            </div>
        </div>
        <div class="row gx-2">
            <div class="col d-xl-none d-lg-block col-lg-2 d-none  me-3 foursquer ">


                <img src="~/images/pin1.png" alt=" " class="" />
                <br />
                <label class="text-muted mt-4">city</label><br />
                <label class=""> @ViewBag.Missiondetail.Cityname</label>
            </div>
            <div class="col d-xl-none d-lg-block col-lg-2 d-none me-3 foursquer ">
                <img src="~/images/web.png" alt=" " class="" />
                <br />
                <label class="text-muted mt-4">Theme </label><br />
                <label class="" name="theme" value="@ViewBag.Missiondetail.MissionId">  @ViewBag.Missiondetail.Themename</label>

            </div>
            <div class="col d-xl-none d-lg-block col-lg-2 d-none  me-3 foursquer ">

                <img src="~/images/calender.png" alt=" " class="" />
                <br />
                <label class="text-muted mt-4">Date  </label><br />
                <label class="">  Ongoing Opportunity</label>

            </div>
            <div class="col d-xl-none d-lg-block col-lg-2 d-none me-3  foursquer ">


                <img src="~/images/organization.png" alt=" " class="" />
                <br />
                <label class="text-muted mt-4">Organization  </label><br />
                <label class=""> @ViewBag.Missiondetail.OrganizationName</label>
            </div>

            <div class="col d-xl-none d-lg-block col-lg-3 d-none me-2 Appleddiv  ">

            @if (ViewBag.Missiondetail.isApplied == true)
            {
                    <button class="aply   mt-4 ms-4   d-flex rounded-5 bg-white px-4 py-2 justify-content-center ">
                        <a class="aplys text-decoration-none">
                            Alredy Aplied

                        </a>
                    </button>


                }
                else if (ViewBag.Missiondetail.isPendding == true)
                {
                    <button class="aply   mt-4 ms-4   d-flex rounded-5 bg-white px-4 py-2 justify-content-center ">
                        <a class="aplys text-decoration-none">
                            Applied Pendding

                        </a>
                    </button>
                }
                else
                {
                    <button class="aply   mt-4 ms-4   d-flex rounded-5 bg-white px-4 py-2 justify-content-center " onclick="missionapplied(@ViewBag.Missiondetail.MissionId,@ViewBag.UserId)">
                        <a class="aplys text-decoration-none">
                            Apply Now
                            <img src="~/images/right-arrow.png" class="mt-0 ms-2" />
                        </a>
                    </button>
                   

           

            }

                </div>


        </div>

        <div class="row mt-3">
            <div class="  col  tabdiv col-12 col-lg-8 shadow-lg ">

                <ul class="nav nav-tabs" id="myTab" role="tablist">
                    <li class="nav-item" role="presentation">
                        <button class="nav-link active" id="mission-tab" data-bs-toggle="tab" data-bs-target="#mission" type="button" role="tab" aria-controls="mission" aria-selected="true">Mission</button>
                    </li>
                    <li class="nav-item" role="presentation">
                        <button class="nav-link" id="org-tab" data-bs-toggle="tab" data-bs-target="#org" type="button" role="tab" aria-controls="org" aria-selected="false">Organization</button>
                    </li>
                    <li class="nav-item" role="presentation">
                        <button class="nav-link" id="contact-tab" data-bs-toggle="tab" data-bs-target="#contact" type="button" role="tab" aria-controls="contact" aria-selected="false">Comments</button>
                    </li>
                </ul>
                <div class="  tab-content" id="myTabContent">
                    <div class="  tab-pane fade show active " id="mission" role="tabpanel" aria-labelledby="mission-tab">
                        <h1>Introduction</h1>
                        <p>@ViewBag.Missiondetail.Description</p>
                        <br /><br />
                        <h1>Challenge</h1>
                        <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur. Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum. Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum.</p>
                        <h1>Documents</h1>
                        <div class="row mb-3">
                            <div class="col  col-11  col-xl-4 col-lg-6 col-md-4 col-sm-11 ms-xl-2  rounded-5 d-flex mt-3  Addfavourite   justify-content-center">
                                <div><img src="~/images/doc.png" /></div>
                                <label class="ms-2">lorem-ipsum.pdf</label>

                            </div>
                            <div class="col col-11 col-xl-4 col-lg-6  col-md-4 col-sm-11 ms-xl-2 rounded-5 d-flex  mt-3 Addfavourite  justify-content-center">
                                <div><img src="~/images/doc.png" /></div>

                                <label class="ms-2"> Recommend to a Co-Worker</label>
                            </div>
                            <div class="col col-11 col-xl-4 col-lg-6  col-md-4 col-sm-11  ms-xl-2 rounded-5 d-flex  mt-3  Addfavourite  justify-content-center">
                                <div><img src="~/images/doc.png" /></div>

                                <label class="ms-2 "> Recommend to a Co-Worker</label>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="org" role="tabpanel" aria-labelledby="org-tab">
                        <p>@ViewBag.Missiondetail.OrganizationDetail</p>

                    </div>
                    <div class="tab-pane fade commentdiv" id="contact" role="tabpanel" aria-labelledby="contact-tab">

                        <div class="form-floating  mt-2">
                            <textarea class="form-control"  placeholder="Leave a comment here" id="floatingTextarea2" style="height: 100px" name="comment"></textarea>
                            <label for="floatingTextarea2">Comments</label>
                        </div>
                        <button type="button" class=" cmtbtn col-8  col-lg-4 col-md-4 px-1  py-2 mt-2 rounded-5 bg-white  align-items-center " onclick="AddPost(@ViewBag.Missiondetail.MissionId,@ViewBag.UserId)">Post Comment</button>

                        <div class=" col-12 bg-light overflow-y-auto border-1 border-black" style="height:500px;">
                            @if (ViewBag.missioncomment == null)
                            {
                                <div> No Comment Found</div>

                            }
                            else
                            {

                                @foreach (var comment in @ViewBag.missioncomment)
                                {
                                    <div class="col-12 d-flex bg-white mt-3  align-items-center">
                                        @if (@comment.Useravtar != "")
                                        {
                                            <img src="@comment.Useravtar" class="  comtuserimg ">
                                        }
                                        else
                                        {
                                            <img src="~/images/user1.png" class="   comtuserimg ">
                                        }
                                      
                                        <div class=" col-9 ms-1 ms-sm-2 ms-ms-4 ms-xl-4  ">
                                            <label class="mx-1 fs-6">@comment.username &nbsp;@comment.lastname</label><br>
                                            <label class="mx-1 fs-6"> @comment.Day &nbsp; @comment.CreatedDate</label>
                                            <div class=" fs-5  text-break "> @comment.Commenttext</div>
                                            </div>

                             
                                        @if (comment.UserId == @ViewBag.UserId)
                                        {

                                            <div class="h-100  ms-auto me-2" id="@comment.Commentid" onclick="cmtdelete(this.id,@ViewBag.Missiondetail.MissionId,@ViewBag.UserId)">
                                            <img src="~/images/bin.png">
                                        </div>
                                        }
                                    </div>
                                }
                            }

                        </div>

                    </div>
                </div>


            </div>
            <div class="col col-12 col-lg-4 flex-lg-column justify-content-center d-md-flex mb-2 ">

                <div class="col col-12 col-lg-12  col-md-6  mt-3 mt-lg-0     shadow-lg">
                    <p class="fs-3 p-2 pb-0">Information</p>
                    <div class="d-flex">
                        <hr class="" style="border:2px solid black; width:230px;" />
                        <hr class="trees" />
                    </div>

                    <div class="d-flex border-bottom border-3 p-2">
                        <div id=" ">
                            Skills
                        </div>
                        <div class="overflow-auto ms-5">
                            Cool, Easy going, Math, Computer

                        </div>
                    </div>
                    <div class="d-flex border-bottom border-3  mt-2 p-2">
                        <div>
                            Days
                        </div>
                        <div class="overflow-auto ms-5">
                            Weekend only
                        </div>
                    </div>
                    <div class="d-flex p-2 mb-2 mt-2">
                        <div>
                            Rating
                        </div>

                        <div class="overflow-auto avgrat   ms-5" id="avgrat">
                            @if (ViewBag.Missiondetail.AvrageRating != 0)
                            {
                                for (int i = 0; i < @ViewBag.Missiondetail.AvrageRating; i++)
                                {
                                    <img src="~/images/selected-star.png" alt="" />
                                }
                                for (int i = @ViewBag.Missiondetail.AvrageRating; i < 5; i++)
                                {
                                    <img src="~/images/star.png" alt="" />
                                }
                            }

                            <div class="mt-1">
                                ( by @ViewBag.ratusercouny volunteers)
                            </div>
                        </div>



                    </div>

                </div>
                <div class="col col-12 col-lg-12  col-md-6   ms-0 ms-lg-0 ms-md-3 mt-3      shadow-lg" id="recent">
                    <div id="recentvolunteer">
                        <p class="fs-3 p-2 pb-0">Recent Volunteers</p>
                        <div class="d-flex">
                            <hr class="" style="border:2px solid black; width:600px;" />
                            <hr class="trees" />
                        </div>
                        @*<div class="container px-0">*@
                            <div class="row   ">

                                @if (@ViewBag.totalcount!=0)
                                {
                                    @foreach (var recent in ViewBag.recentvolunteered)
                                    {
                                        <div class="col-6  col-xl-4 col-sm-4  h-auto ">
                                            <a class=" ms-2 me-2 text-decoration-none text-black " asp-area="" asp-controller="home" asp-action="login">
                                                @if (@recent.Useravtar != "")
                                                {
                                                    <img src="@recent.Useravtar" class="involveduserprofile"  />
                                                }
                                                else
                                                {
                                                    
                                                
                                                <img src="~/images/user1.png" class="involveduserprofile  " />
                                                }
                                                <label id="" class="text-center d-flex  justify-content-center">@recent.username</label>
                                            </a>
                                        </div>
                                    }
                                }
                                else
                                {
                                    <span class="fs-4" > No one Recentvolunteered Found</span>
                                }



                            </div>
                       


                        <hr class="my-0"/>
                        @if (pagination.TotalPages > 0)
                        {
                            <ul class="pagination d-flex justify-content-between mb-0 pe-0 mt-0">
                                @if (pagination.CurrentPage > pagination.StartPage)
                                {

                                    <li class="page-item ">
                                        <a class="page-link " style="background-color:white;" onclick="recentvol(@pagination.CurrentPage - 1,@ViewBag.Missiondetail.MissionId,@ViewBag.UserID)"><img src="~/images/left.png" /></a>
                                    </li>

                                }
                                else
                                {
                                    <li class="page-item ">
                                        <a class="page-link " style="background-color:white;"><img src="~/images/left.png" /></a>
                                    </li>
                                }
                                <li class="page-item d-flex justify-content-center align-items-center ">
                                    <span class=""> Recent @ViewBag.totalcount Pepole Voluntered</span>
                                </li>


                                @if (pagination.CurrentPage < pagination.TotalPages)
                                {
                                    <li class="page-item">
                                        <a class="page-link" style="background-color:white" onclick="recentvol(@pagination.CurrentPage + 1,@ViewBag.Missiondetail.MissionId,@ViewBag.UserID)"><img src="~/images/right-arrow1.png" /></a>
                                    </li>

                                }
                                else
                                {
                                    <li class="page-item">
                                        <a class="page-link" style="background-color:white;"><img src="~/images/right-arrow1.png" /></a>
                                    </li>
                                }
                            </ul>
                        }


                     


                    </div>
                </div>
            </div>
        </div>

    </div>


    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="~/js/Missionpage.js" asp-append-version="true"></script>


</body>
</html>

